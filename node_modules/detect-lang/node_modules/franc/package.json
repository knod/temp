  "devDependencies": {
    "browserify": "^10.0.0",
    "eslint": "^0.23.0",
    "esmangle": "^1.0.0",
    "iso-639-3": "^0.2.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.0.0",
    "jscs-jsdoc": "^1.0.0",
    "markdown-table": "^0.4.0",
    "matcha": "^0.6.0",
